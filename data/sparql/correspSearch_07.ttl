# Schauspieler an Weber

PREFIX rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:<http://www.w3.org/2000/01/rdf-schema#>
PREFIX tei:<http://www.tei-c.org/ns/1.0>
PREFIX foaf:<http://xmlns.com/foaf/0.1/>
PREFIX owl:<http://www.w3.org/2002/07/owl#>
PREFIX owltime:<http://www.w3.org/2006/time#>
PREFIX rel:<http://purl.org/vocab/relationship/>
PREFIX gndo: <http://d-nb.info/standards/elementset/gnd#>
PREFIX cd: <http://www.tei-c.org/ns/1.0/correspDesc#>

SELECT DISTINCT ?name
WHERE {
  	?sender cd:sending ?letter ;
			gndo:professionOrOccupation ?occupationURI ;
    		rdfs:label ?name .
  	?occupationURI rdfs:label ?occupation .
  	FILTER(?occupation = "Schauspieler"@de) .
  	<http://d-nb.info/gnd/118629662> cd:receiving ?letter .
}
ORDER BY ASC(?name)