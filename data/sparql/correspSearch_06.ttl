# Briefe von Goethe aus Weimar

PREFIX rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:<http://www.w3.org/2000/01/rdf-schema#>
PREFIX tei:<http://www.tei-c.org/ns/1.0>
PREFIX foaf:<http://xmlns.com/foaf/0.1/>
PREFIX owl:<http://www.w3.org/2002/07/owl#>
PREFIX owltime:<http://www.w3.org/2006/time#>
PREFIX rel:<http://purl.org/vocab/relationship/>
PREFIX cd: <http://www.tei-c.org/ns/1.0/correspDesc#>

SELECT DISTINCT ?letter ?name
WHERE {
  	<http://d-nb.info/gnd/118540238> cd:sending ?letter .
  	?recipient cd:receiving ?letter .
  	?recipient rdfs:label ?name .
  	?letter cd:sentFrom <http://www.geonames.org/2812482>
}