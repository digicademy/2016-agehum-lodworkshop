PREFIX gn: <http://www.geonames.org/ontology#>
PREFIX gndo: <http://d-nb.info/standards/elementset/gnd#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX dracula: <http://www.graphdracula.net/>
SELECT DISTINCT ?subject ?subjectLabel ?object ?objectLabel (strafter(str(?predicate), '#') AS ?localName) ?sourceColor ?targetColor
WHERE {
  <http://d-nb.info/gnd/118540238> ?predicate ?object .
  ?subject ?predicate ?object .
  ?subject rdfs:label ?subjectLabel .
  ?object rdfs:label ?objectLabel .
  ?subject rdf:type ?subjectType .
  ?subjectType dracula:nodeColor ?sourceColor .
  ?object rdf:type ?objectType .
  ?objectType dracula:nodeColor ?targetColor .
  MINUS { ?subject rdf:type ?object }
  MINUS { ?subject gndo:familialRelationship ?object }
}